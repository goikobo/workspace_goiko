import java.util.Scanner;


public class condicionales {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1 = 5;
		int num2 = 6;
		float resultado = 0;
		char op = '+';
		
		switch (op){
		case '+':
				resultado = num1 + num2;
				break;
		case '-':
				resultado = num1 - num2;
				break;
		case '*':
				resultado = num1 * num2;
				break;
		case '/':
				resultado = num1 / num2;
				break;
					
		}

		System.out.println("El resultado es: "+ resultado);	
	}

}
